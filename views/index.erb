<div id="main-container">
    <div id="LoginForm">
            <div class="login-form">
                <div class="main-div">
                    <div class="panel">
                        <h2>Admin Login</h2>
                        <p>Bitte E-Mail und Password eingeben</p>
                    </div>
                    <form id="Login" action='backend' method='post'>
                        <div class="form-group">
                            <input type="email" class="form-control" id="inputEmail" name="inputEmail" placeholder="E-Mail Addresse">
                        </div>
                        <div class="form-group">
                            <input type="password" class="form-control" id="inputPassword" name="inputPassword" placeholder="Passwort">
                        </div>
                            <button type="submit" class="btn btn-primary">Login</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

